<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>   
        <property name="show_sql">false</property>
        <mapping resource="persistence/Account.hbm.xml" />
        <mapping resource="persistence/Menu.hbm.xml" />
        <mapping resource="persistence/Options.hbm.xml" />
        <mapping resource="persistence/Profile.hbm.xml" />
        <mapping resource="persistence/Users.hbm.xml" />
        
    </session-factory>
</hibernate-configuration>
